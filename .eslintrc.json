{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "angular",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            2,
            2
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 1,
        "angular/controller_name": 0,
        "no-use-before-define": 0
    }
}